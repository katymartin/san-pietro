{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "801f5d97-e17c-4df0-82a1-6047cb5f92eb": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.my_fields.flavor_profiles | metafield_text }}",
            "text_style": "flavor-profiles"
          }
        },
        "0bd9903e-96d8-4f95-b561-df3529ea1fa3": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.my_fields.pairs_with | metafield_text }}",
            "text_style": "pairs-with"
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "collapsible-content-2": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Local Delivery",
            "icon": "truck",
            "content": "<p>Now offering same day delivery to Tuckahoe, Bronxville, and Mount Vernon. Orders usually arrive within one hour of purchase.<\/p>",
            "page": ""
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "price",
        "801f5d97-e17c-4df0-82a1-6047cb5f92eb",
        "0bd9903e-96d8-4f95-b561-df3529ea1fa3",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "collapsible-content-2"
      ],
      "settings": {
        "enable_sticky_info": true,
        "gallery_layout": "stacked",
        "media_size": "medium",
        "mobile_thumbnails": "hide",
        "hide_variants": false,
        "enable_video_looping": false,
        "padding_top": 24,
        "padding_bottom": 60
      }
    },
    "main-product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "background-1",
        "image_ratio": "adapt",
        "show_secondary_image": false,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 24
      }
    }
  },
  "order": [
    "main",
    "main-product-recommendations"
  ]
}
